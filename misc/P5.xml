<Segment name="P5" comment="To indicate port-related data">
  <Field name="PortOrTerminalFunctionCode" min="1" max="1" validation="T115" comment="Code defining function performed at the port or terminal with respect to a shipment"/>
  <Field name="LocationQualifier" min="1" max="2" validation="T309" comment="Code identifying type of location"/>
  <Field name="LocationIdentifier" min="1" max="30" comment="Code which identifies a specific location"/>
</Segment>
