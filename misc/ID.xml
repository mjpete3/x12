  <Field name="DamageAreaCode" min="2" max="2" comment="Code representing the location on the vehicle where the damage was repaired"/>
  <Field name="DamageTypeCode" min="2" max="2" comment="Code indicating the type of damage"/>
  <Field name="DamageSeverityCode" min="1" max="1" comment="Code indicating the severity of the damage"/>
</Segment>
